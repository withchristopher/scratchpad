<div class="row">
    <div class="col s12 m6">
        <div class="card blue-grey">
        <div class="card-content white-text">
            <span class="card-title">Create new account:</span>
    <form class="signup-form">
        <div>
            <label class="card-content white-text" for="username-signup">username:</label>
            <input type="text" id="username-signup" />
        </div>
        <div>
            <label class="card-content white-text" for="email-signup">email:</label>
            <input type="text" id="email-signup" />
        </div>
        <div>
            <label class="card-content white-text" for="twitter-signup">twitter:</label>
            <input type="text" id="twitter-signup" />
        </div>
        <div>
            <label class="card-content white-text" for="github-signup">github:</label>
            <input type="text" id="github-signup" />
        </div>
        <div>
            <label class="card-content white-text" for="password-signup">password:</label>
            <input type="password" id="password-signup" />
        </div>
        <div>
            <button type="submit" class="btn">signup</button>
        </div>
    </form>
</div>

<!-- javascript file-->
<script src="/javascript/signup.js"></script>